{
	"schemaVersion": 1,
	"id": "enchanted-book-press",
	"version": "${version}",
	"name": "Enchanted Book Press",
	"description": "Adds a Copying Table for cloning enchanted books!",
	"authors": [
		"https://github.com/GravyJonesGalleon"
	],
	"contact": {
		"sources": "https://github.com/GravyJonesGalleon/enchanted-book-press"
	},
	"license": "CC0-1.0",
	"icon": "assets/enchanted-book-press/textures/block/printing_press_top.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"dev.gravy.enchantedpress.EnchantedBookPress"
		],
		"client": [
			"dev.gravy.enchantedpress.EnchantedBookPressClient"
		],
		"fabric-datagen": [
			"dev.gravy.enchantedpress.EnchantedBookPressDataGenerator"
		]
	},
	"mixins": [
		"enchanted-book-press.mixins.json",
		{
			"config": "enchanted-book-press.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21.11",
		"java": ">=21",
		"fabric-api": "*"
	}
}